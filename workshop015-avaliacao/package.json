{
  "name": "ledgerdary-node",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "main": "src/index.js",
  "homepage": "https://gitlab.com/douglas.marques/ledgerdary-node#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/douglas.marques/ledgerdary-node.git"
  },
  "scripts": {
    "dev": "tsnd --transpile-only --files src/index.ts | pino-pretty -c -l",
    "db:create:test": "createdbjs ${TEST_DATABASE_DB} --user=${TEST_DATABASE_USER} --password=${TEST_DATABASE_PASSWORD} --host=${TEST_DATABASE_HOST}",
    "db:create:dev": "createdbjs ${DATABASE_DB} --user=${DATABASE_USER} --password=${DATABASE_PASSWORD} --host=${DATABASE_HOST}",
    "db:migrate:test": "knex migrate:latest --env=test",
    "db:migrate:dev": "knex migrate:latest",
    "db:migrate:dev:create": "knex migrate:make",
    "db:rollback:test": "knex migrate:rollback --env=test",
    "db:rollback:dev": "knex migrate:rollback",
    "db:seed:dev": "knex seed:run",
    "docker:install": "docker-compose run app npm i",
    "docker:db:create:test": "docker-compose run app bash -c \"npm run db:create:test\"",
    "docker:db:create:dev": "docker-compose run app bash -c \"npm run db:create:dev\"",
    "docker:db:migrate:test": "docker-compose run test npm run db:migrate:test",
    "docker:db:migrate:dev": "docker-compose run app npm run db:migrate:dev",
    "docker:db:rollback:test": "docker-compose run test npm run db:rollback:test",
    "docker:db:rollback:dev": "docker-compose run app npm run db:rollback:dev",
    "docker:db:seed:dev": "docker-compose run app npm run db:seed:dev",
    "docker:test": "docker-compose run test npm test",
    "docker:dev": "docker-compose up",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "body-parser": "1.20.1",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "knex": "2.3.0",
    "objection": "3.0.1",
    "pg": "8.8.0",
    "pgtools": "0.3.2",
    "pino": "8.7.0",
    "pino-http": "8.2.1",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/chance": "1.1.3",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.4",
    "@types/knex-cleaner": "1.3.2",
    "@types/node": "18.11.11",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "chance": "1.1.9",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.1.7",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "fishery": "2.2.2",
    "jest": "29.3.1",
    "knex-cleaner": "1.3.1",
    "pino-pretty": "9.1.1",
    "prettier": "2.8.1",
    "supertest": "6.3.3",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.3"
  }
}
