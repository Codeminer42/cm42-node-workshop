    "webtorrent": "2.1.37",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.21",
    "@types/webtorrent": "^0.109.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "pino-pretty": "^10.3.1",
